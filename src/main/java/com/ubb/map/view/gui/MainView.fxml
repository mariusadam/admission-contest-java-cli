<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="stackPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ubb.map.view.gui.MainController">
   <children>
      <BorderPane fx:id="borderPane">
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" visible="false" />
               </children>
            </StackPane>
         </center>
         <top>
            <MenuBar BorderPane.alignment="CENTER">
               <menus>
                  <Menu mnemonicParsing="false" text="Session">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Login as other user" />
                        <MenuItem mnemonicParsing="false" text="Logout and exit" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Admission">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Manage Options" />
                        <MenuItem mnemonicParsing="false" text="Manage Candidates" />
                        <MenuItem mnemonicParsing="false" text="Manage Departments" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Administration">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Manage Users" />
                        <MenuItem mnemonicParsing="false" text="Manage Roles" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
         </top>
      </BorderPane>
      <VBox fx:id="loginPane" prefHeight="200.0" prefWidth="100.0" StackPane.alignment="TOP_LEFT">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <VBox alignment="CENTER" prefWidth="300.0">
                     <children>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Text fx:id="loginMessage" strokeType="OUTSIDE" strokeWidth="0.0" text="Some error mesage  at login" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Pane>
                        <GridPane>
                           <children>
                              <Label text="Email:" />
                              <TextField fx:id="emailTextField" prefHeight="25.0" prefWidth="181.0" GridPane.columnIndex="1" />
                           </children>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="127.0" minWidth="10.0" prefWidth="91.0" />
                              <ColumnConstraints hgrow="SOMETIMES" prefWidth="177.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                        </GridPane>
                        <GridPane>
                           <children>
                              <TextField fx:id="passwordTextField" prefHeight="25.0" prefWidth="125.0" GridPane.columnIndex="1" />
                              <Label text="Password:" />
                           </children>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="128.0" minWidth="10.0" prefWidth="90.0" />
                              <ColumnConstraints hgrow="SOMETIMES" prefWidth="177.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </GridPane>
                        <Button mnemonicParsing="false" onAction="#onLoginBtn_clicked" text="Login">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</StackPane>
